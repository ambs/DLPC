         <entry type="simpleWord" xml:lang="pt" xml:id="Lobo">
            <form type="lemma">
               <orth>lobo</orth>
               <pron>lóbu</pron>
               <lbl>:1</lbl>
            </form>
            <gramGrp>
               <gram type="pos" norm="NOUN">s.</gram>
               <gram type="gen">m.</gram>
            </gramGrp>
            <etym type="inheritance">
               <seg type="desc">Do</seg>
               <cit type="etymon" xml:lang="la">
                  <form><orth>lupus</orth>, relativo ao animal em que Júpiter transformou Licaão colocando-o no céu, entre as estrelas.</form>
               </cit>
            </etym>
            <sense xml:id="Lobo_1" n="1">
               <usg type="domain">Astr.</usg>
               <def>Constelação austral, que ocupa 334 graus quadrados, formado por estrelas de magnitude média.</def>
            </sense>
